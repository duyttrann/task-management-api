{
  "name": "task-management-api",
  "version": "1.0.0",
  "description": "APIs for clone app",
  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint src",
    "clean": "del /s build && mkdir build",
